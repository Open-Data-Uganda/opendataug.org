{
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "bracketSameLine": true,
  "printWidth": 120,
  "htmlWhitespaceSensitivity": "strict",
  "proseWrap": "preserve",
  "plugins": ["prettier-plugin-tailwindcss"]
}
